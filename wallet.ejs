              <th>SEND TO</th>
              <th>RECEIVER MOBILE NUMBER</th>
              <th>AMOUNT</th>
          </tr>
        </thead>
        <tbody>
         <% for(var i=0; i < sentLength; i++) { %> 
           <tr>
            <td><%= sentRow[i].to_name %></td>
            <td><%= sentRow[i].to_mobile %></td>
            <td><%= sentRow[i].amount %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
      <hr>
      <hr>
      <table>
